all: %

ex_binarytree: ex_binarytree.c
	gcc -Wall -O0 -o $@ $<

ordering: ordering.c
	gcc -Wall -O0 -o $@ $<

calculator: calculator.c
	gcc -Wall -O0 -o $@ $<

dic: dic.c
	gcc -Wall -O0 -o $@ $<

BST_demo: BST_demo.c
	gcc -g -Wall -O0 -o $@ $<

heap: heap.c
	gcc -Wall -O0 -o $@ $<
 
clean:
	rm ex_binarytree ordering calculator dic BST_demo heap

